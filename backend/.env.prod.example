# Copy to .env.prod and fill in production values (DO NOT commit real secrets)
# Required:
DATABASE_URL=mysql+pymysql://<db_user>:<db_password>@<host>:3306/<db_name>?charset=utf8mb4
SECRET_KEY=replace-with-secure-random-string
JWT_SECRET_KEY=replace-with-secure-random-string
# Optional:
ENABLE_DEBUG_ENDPOINTS=0
DEV_SQLITE_FALLBACK=0
DEBUG=0
TESTING=0
UPLOAD_FOLDER=/var/app/uploads
